{% extends "base.html.twig" %}

{% block title %}
	Création d'une catégorie
{% endblock title %}

{% block body %}
	<h1>Nouvelle Catégorie</h1>

	{{ form_start(formView) }}

	{{ form_errors(formView) }}

	{{ form_widget(formView) }}
	
	<button type="submit" class="btn btn-primary mt-2">
		<i class="fas fa-save"></i>
		Créer la catégorie
	</button>

	{{ form_end(formView) }}
{% endblock body %}